    "file-loader": "^0.8.1",
    "imports-loader": "^0.6.3",
    "jasmine": "^2.2.1",
    "jasmine-core": "^2.2.0",
    "jshint": "^2.6.0",
    "jsx-loader": "^0.12.2",
    "jsxhint": "^0.10.0",
    "karma": "^0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.5.0",
